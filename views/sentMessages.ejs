<%- include("partials/header"); %>

    <h1>Sent Messages</h1>

    <table class="table table-bordered table-responsive">
        <thead>
            <tr>
                <th scope="col">Name</th>
                <th scope="col">Date</th>
                <th scope="col">Time</th>
                <th scope="col">OTP</th>
            </tr>
        </thead>
        <tbody>
            <% logs.forEach(log=> { %>
                <tr>
                    <td><%= log.name %></td>
                    <td><%= log.date %></td>
                    <td><%= log.time %></td>
                    <td><%= log.otp %></td>
                </tr>
            <% }); %>
        </tbody>
        </tbody>
    </table>

    <%- include("partials/footer"); %>